{
  "name": "@autoviews/core",
  "version": "1.0.3",
  "description": "TODO",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:wix-incubator/autoviews.git",
    "directory": "packages/core"
  },
  "homepage": "https://github.com/wix-incubator/autoviews/tree/master/packages/core#readme",
  "scripts": {
    "clear": "yarn run -T rimraf ./dist",
    "build": "yarn run -T run-s clear build:ts",
    "build:ts": "yarn run -T tsc -p tsconfig.cjs.json && yarn run -T tsc -p tsconfig.esm.json",
    "lint": "yarn run -T eslint \"**/*.{js,ts}?(x)\"",
    "test": "yarn run -T run-s lint test:jest",
    "test:jest": "yarn run -T jest --maxWorkers=50%",
    "test:watch": "yarn run -T jest --watch --runInBand",
    "test:clear": "yarn run -T jest --clearCache",
    "release": "yarn run -T semantic-release -e semantic-release-monorepo"
  },
  "dependencies": {
    "ajv": "6.12.6",
    "fast-json-patch": "3.1.0",
    "json-pointer": "0.6.2"
  },
  "peerDependencies": {
    "lodash": "^4.17.4",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "author": "AutoViews Team (https://github.com/wix-incubator/autoviews)",
  "license": "MIT"
}
